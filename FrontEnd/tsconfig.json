{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "includes": [
    "src/**/*.ts",
    "src/**/*.vue",
    "src/types/*.d.ts"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "moduleResolution": "node"
  },
  "typeRoots": ["./node_modules/@types", "types"]
}
